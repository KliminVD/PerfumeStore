<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/firstFragment2">

    <fragment
        android:id="@+id/firstFragment2"
        android:name="com.example.courseproject.Fragments.AddProductFragment"
        android:label="First Fragment"
        tools:layout="@layout/fragment_add_product">
    </fragment>
    <fragment
        android:id="@+id/secondFragment2"
        android:name="com.example.courseproject.Fragments.SettingsFragment"
        android:label="Second Fragment"
        tools:layout="@layout/fragment_settings">
    </fragment>
    <fragment
        android:id="@+id/thirdFragment2"
        android:name="com.example.courseproject.Fragments.AuthorFragment"
        android:label="Third Fragment"
        tools:layout="@layout/fragment_author">
    </fragment>
    <action
        android:id="@+id/action_global_navigation_home2"
        app:destination="@id/firstFragment2" />

    <action
        android:id="@+id/action_global_navigation_dashboard2"
        app:destination="@id/secondFragment2" />

    <action
        android:id="@+id/action_global_navigation_notifications2"
        app:destination="@id/thirdFragment2" />

</navigation>
